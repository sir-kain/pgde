{% set dmd = 'dmd' %}
{% if settings.route == 'admin_pgde_emploi_userdata_list' %}
    {% set dmd = path(settings.route) %}
{% else %}
    {% set dmd = path(settings.route) ~ '?dmd=dmdinv' %}
{% endif %}

<div id="cms-block-{{ block.id }}" class="cms-block cms-block-element">
    <div class="small-box {{ settings.css_class }}">
        <div class="inner">
            <h3>{{ count }}</h3>

            <p>{{ settings.title }}</p>
        </div>
        <div class="icon">
            <i class="fa {{ settings.icon }}"></i>
        </div>
        <a href="{{ dmd }}" class="small-box-footer">
            voir plus <i class="fa fa-arrow-circle-right" aria-hidden="true"></i>
        </a>
    </div>
</div>